{
  "name": "gitbook-plugin-myseo2",
  "version": "1.0.2",
  "description": "This is a GitBook plugin for generate keywords and description for SEO (optimzed)",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies":{
	"html-entities":""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sy2729/gitbook-plugin-myseo2.git"
  },
  "keywords": [
    "seo",
    "keywords"
  ],
  "author": "wenki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sy2729/gitbook-plugin-myseo2/issues"
  },
  "homepage": "https://github.com/sy2729/gitbook-plugin-myseo2",
  
  "engines":{
        "gitbook":">1.x.x"
    },
	"gitbook":{
		"properties":{
			"titlePrefix":{
				"type":"string",
				"default":"",
				"description":"It defines your title prefix"
			},
			"titleSuffix":{
				"type":"string",
				"default":"",
				"description":"It defines your title suffix"
			},
			"titleAfterfix":{
				"type":"string",
				"default":"",
				"description":"It defines your title suffix that's replaceble by individual in-article title element"
			},
			"description":{
				"type":"string",
				"default":"",
				"description":"It defines your description"
			},
			"keywords":{
				"type":"string",
				"default":"",
				"description":"It defines your keywords"
			}
		}
	}
}
